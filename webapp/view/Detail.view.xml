<mvc:View
	controllerName="sap.ui.demo.staff.controller.Detail"
	xmlns="sap.m"
	xmlns:f="sap.f"
	xmlns:mvc="sap.ui.core.mvc">
	<Page
		title="{i18n>detailPageTitle}"
		showNavButton="true"
		navButtonPress=".onNavBack">
		<ObjectHeader
			icon="sap-icon://customer"
			title="{Info>FirstName} {Info>LastName}"
			intro="{Info>Position}" >
			<attributes>
				<ObjectAttribute title="{i18n>UserID}" text="{Info>UserID}" />
				<ObjectAttribute title="{i18n>Login}" text="{Info>Login}" />
				<ObjectAttribute title="{i18n>CreatedDate}" text="{Info>CreatedDate}" />
				<ObjectAttribute title="{i18n>DateBirth}" text="{Info>DateBirth}" />
				<ObjectAttribute title="{i18n>Project}" text="{Info>Project}" />
				<ObjectAttribute title="{i18n>Manager}" text="{Info>Manager}" />
				<ObjectAttribute title="{i18n>Room}" text="{Info>Room}" />
				<ObjectAttribute title="{i18n>Computer}" text="{Info>Computer}" />
			</attributes>
		</ObjectHeader>
		<footer>
				<Toolbar>
				<content/>
			</Toolbar>
		</footer>
	</Page>
</mvc:View>

