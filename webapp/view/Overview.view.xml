<mvc:View
		controllerName="sap.ui.demo.staff.controller.Overview"
		xmlns="sap.m"
		xmlns:mvc="sap.ui.core.mvc"
		xmlns:core="sap.ui.core">
	<Page title="{i18n>homePageTitle}">
		<headerContent>
			<Button
					icon="sap-icon://sys-help-2"
					press=".onOpenHelpDialog"/>					
		</headerContent>
		<content>
			<core:Fragment id="usersList" fragmentName="sap.ui.demo.staff.view.UsersList" type="XML"/>		
		</content>
			<footer>
				<Toolbar>
					<content>
						<ToolbarSpacer/>
						<Button
						text="Add new User"
						icon="sap-icon://add-contact"
						press=".onOpenAddUserDialog"
						type="Accept"/> 
				</content>
			</Toolbar>
		</footer>
	</Page>
</mvc:View>